{
    "name": "nette/bundle",
    "description": "The sandbox is a pre-packaged Nette Framework project, basic configured structure for your application.",
    "type": "project",
    "license": [
        "BSD-3-Clause",
        "GPL-2.0",
        "GPL-3.0"
    ],
    "authors": [
        {
            "name": "Tomas Reinert",
            "email": "reinerttomas@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "nette/application": "^3.0",
        "nette/bootstrap": "^3.0",
        "nette/caching": "^3.0",
        "nette/di": "^3.0",
        "nette/finder": "^2.5",
        "nette/forms": "^3.0",
        "nette/http": "^3.0",
        "nette/mail": "^3.0",
        "nette/robot-loader": "^3.0",
        "nette/safe-stream": "^2.4",
        "nette/security": "^3.0",
        "nette/utils": "^3.0",
        "latte/latte": "^2.5",
        "tracy/tracy": "^2.6",
        "dg/adminer-custom": "^1.8",
        "contributte/application": "^0.4.0",
        "contributte/cache": "^0.5.0",
        "contributte/latte": "^0.4.1",
        "contributte/utils": "^0.4.2",
        "contributte/mail": "^0.5.1",
        "contributte/di": "^0.4.2",
        "ublaboo/datagrid": "^6.2",
        "contributte/console": "^0.8.0",
        "contributte/console-extra": "^0.5.2",
        "contributte/event-dispatcher": "^0.7.0",
        "contributte/event-dispatcher-extra": "^0.7.1",
        "nettrine/dbal": "^0.6.2",
        "nettrine/orm": "^0.5.0",
        "nettrine/annotations": "^0.6.0"
    },
    "require-dev": {
        "nette/tester": "^2.0",
        "phpstan/phpstan": "^0.12.0",
        "phpstan/phpstan-doctrine": "^0.12.0",
        "phpstan/phpstan-deprecation-rules": "^0.12.0",
        "phpstan/phpstan-nette": "^0.12.0",
        "phpstan/phpstan-phpunit": "^0.12.0",
        "phpstan/phpstan-strict-rules": "^0.12.0"
    },
    "autoload": {
        "classmap": [
            "app/Bootstrap.php"
        ],
        "psr-4": {
            "App\\Model\\": "app/model",
            "App\\Domain\\": "app/domain",
            "App\\UI\\": "app/ui",
            "Database\\": "db",
            "App\\Modules\\": "app/modules"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "platform": {
            "php": "7.4"
        }
    },
    "scripts": {
        "phpstan": "phpstan analyse --level 5 --configuration phpstan.neon app bin www"
    }
}
